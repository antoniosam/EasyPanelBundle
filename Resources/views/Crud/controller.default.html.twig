<?php

namespace {{ namespace }};

use Symfony\Bundle\FrameworkBundle\Controller\AbstractController;
use Sensio\Bundle\FrameworkExtraBundle\Configuration\Route;
use Ast\EasyPanelBundle\Services\EasyPanelService;

{%  if prefix_route_controller is not empty %}
/**
 * @Route("{{ prefix_route_controller }}" )
 */
 {% endif %}
class DefaultController extends AbstractController
{

    /**
     * @Route("/", name="{{ prefix_route_name }}home")
     */
    public function indexAction({{ serviceautowire }})
    {
        return {{ serviceincontroller|raw }}->renderTestHome();
    }

    /**
     * @Route("/dashboard", name="{{ prefix_route_name }}dashboard")
     */
    public function dashboardAction({{ serviceautowire }})
    {
        $this->get('session')->set('menu-active', 'dashboard');
        return {{ serviceincontroller|raw }}->renderTestDashboard();
    }

}
