<div class="sidebar" data-color="azure" data-background-color="white" >
    <div class="logo">
        <a href="{{ "{{ path('"}}{{ prefix }}{{ "_default_home') }}" }}" class="simple-text logo-normal">
            {{ proyecto }}
        </a>
    </div>
    <div class="sidebar-wrapper">
        <ul class="nav">
            <li class="nav-item active  ">
                <a class="nav-link" href="{{ "{{ path('"}}{{ prefix }}{{ "_default_dashboard') }}" }}">
                    <i class="material-icons">dashboard</i>
                    <p>Dashboard</p>
                </a>
            </li>
            {% for ruta in rutas %}
                <li class="nav-item"  title="{{ ruta[0]|capitalize }}">
                    <a class="nav-link" href="{{ "{{ path('"}}{{ ruta[1] }}{{ "_index') }}"}}">
                        <i class="fa {{ ruta[2] }}"></i>
                        <p >{{ ruta[0]|capitalize }}</p>
                    </a>
                </li>
            {% endfor %}

        </ul>
    </div>
    <div class="sidebar-background" style="background-image: url('{{ "{{asset('admin/assets/material') }}" }}/img/sidebar-3.jpg');"></div>
</div>