<script>

$(document).ready(function() {
    $(".frm-search-table .input-search").on("keyup",function(){
        var close = $(this).parent().parent().find('.close-search');
        if($.trim($(this).val())!=''){
            if(!$(close).is(':visible')){
                $(close).fadeIn();
            }
        }else{
            $(close).fadeOut();
        }
    });
    $('form').each(function (){
        var form =  $(this);
        var has_summernote =  form.find('.initsummernote').length > 0;
        if(has_summernote){
            form.find('.initsummernote').each(function(obj,callback){
                var textarea = $(this);
                var id = textarea.attr('id');
                textarea.attr('style','height:1px!important; padding:0px;');
                textarea.parent().find('label[for='+id+']').each(function(){
                    $(this).removeClass('bmd-label-floating');
                    $(this).removeClass('bmd-label-static');
                    setTimeout(()=>{
                        $(this).removeClass('bmd-label-static');
                    },200);
                });
                var div = $('<div>');
                div.attr('id',id+'summernote');
                div.insertAfter(textarea);
                $('#'+id+'summernote').summernote({
                    toolbar:[
                        ['style', ['style']],
                        ['font', ['bold', 'underline', 'clear']],
                        ['fontname', ['fontname']],
                        ['color', ['color']],
                        ['para', ['ul', 'ol', 'paragraph']],
                        ['table', ['table']],
                        ['insert', ['link', 'picture-upload']],
                        ['view', ['codeview', 'help']]
                    ],
                    height: 200,
                    tabsize: 2,
                    focus: false
                });
            });
            form.on('submit',function(e){
                $(this).find('.initsummernote').each(function(obj,callback){
                    var textarea = $(this);
                    var id = textarea.attr('id');
                    textarea.val($('#'+id+'summernote').summernote('code'));
                });
                var button = $(this).find('button[type=submit]');
                var  icon = button.find('i.fa');
                icon.removeClass('fa-floppy-o');
                icon.addClass('fa-refresh fa-spin');
                button.attr('disabled','disabled');
                button.add('disabled');
            });
        }else{
            form.on('submit',function(e){
                var button = $(this).find('button[type=submit]');
                var  icon = button.find('i.fa');
                icon.removeClass('fa-floppy-o');
                icon.addClass('fa-refresh fa-spin');
                button.attr('disabled','disabled');
                button.add('disabled');
            });
        }
    });
});


</script>
